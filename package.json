{
  "name": "youtube-search-filter",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "preinstall": "typesync || :",
    "watch": "node ./node_modules/webpack/bin/webpack.js --mode=development --watch --config config/webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --mode=production --config config/webpack.config.js",
    "build:zip": "node ./node_modules/webpack/bin/webpack.js --mode=production --config config/webpack.config.js && zip -r youtube-search-filter.zip build/",
    "format": "prettier --write 'src/**/*.ts' && eslint ./src/",
    "zip": "zip -r youtube-search-filter.zip build/"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.203",
    "@types/copy-webpack-plugin": "^6.4.3",
    "@types/eslint": "^8.4.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/file-loader": "^5.0.1",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.1",
    "prettier": "^2.8.0",
    "size-plugin": "^2.0.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.3",
    "typesync": "^0.9.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
